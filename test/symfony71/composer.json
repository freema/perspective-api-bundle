{
  "name": "freema/perspective-api-bundle-test-71",
  "description": "Test environment for Symfony 7.1",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "symfony/http-kernel": "7.1.*",
    "symfony/framework-bundle": "7.1.*",
    "symfony/yaml": "7.1.*",
    "symfony/console": "7.1.*",
    "symfony/http-client": "7.1.*",
    "symfony/cache": "7.1.*",
    "symfony/validator": "7.1.*"
  },
  "require-dev": {
    "symfony/debug-bundle": "7.1.*",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Freema\\PerspectiveApiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freema\\PerspectiveApiBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}