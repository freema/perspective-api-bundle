{
  "name": "freema/perspective-api-bundle-test-64",
  "description": "Test environment for Symfony 6.4",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "6.4.*",
    "symfony/framework-bundle": "6.4.*",
    "symfony/yaml": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/http-client": "6.4.*",
    "symfony/cache": "6.4.*",
    "symfony/validator": "6.4.*"
  },
  "require-dev": {
    "symfony/debug-bundle": "6.4.*",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Freema\\PerspectiveApiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freema\\PerspectiveApiBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}